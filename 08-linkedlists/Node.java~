import java.io.*;
import java.util.*;

public class Node{

    private String data;
    private Node next;

    public Node (String d){
	data = d;
	next = null;

    }

    public void setData(String d){
	data = d;
    }

    public void getData(){
	return data;
    }

    public void setNext(Node n){
	next = n;
    }

    public void getNext(){
	return next;
    }

    public String toString(){
	return data;
    }
}